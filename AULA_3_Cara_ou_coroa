import random

def double_letter(s): return ''.join([c*2 for c in s])
    
def secret_function(a,b):
    if isinstance(a,int) and isinstance(b,int): return a+b
    elif isinstance(a,str) and isinstance(b,str): return a+b
    return "Tipos incompatÃ­veis!"
def cara_ou_coroa(): return random.choice(["cara","coroa"])
